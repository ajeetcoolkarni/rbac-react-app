# RBAC React App Environment Variables (sample)
# Copy to .env and adjust values as needed.

# Base URL for API mode
REACT_APP_API_BASE_URL=http://localhost:3001/api

# Use demo static data (true/false). Set false in production.
REACT_APP_RBAC_SIMPLE_DEMO=true

# Optional default role/user for initial permission load
REACT_APP_RBAC_DEFAULT_ROLE_ID=1
